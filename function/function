#include<stdio.h>
#include<conio.h>
int main()
{
    int No=0,i=0;
    printf("\n Enter +ve integer number ");
    scanf("%d",&No);
    if(No<0)
    {
        printf("\n INVALID INPUT");
    }
    else if (No==0||No==1)
    {
        printf("|n number is noutral");
    }
    else
    {
        for(i=2;i<No;i++)
            if(No%i==0)
        {
            break;
        }
    }
    if(i==No)
    {
        printf("\n given number %d is prime",No);
    }
    else
    {
        printf("\n given number %d is prime",No);
    }
    printf("\n\n=====================thank you=======================\n");
    getch();
    return 0;
}
