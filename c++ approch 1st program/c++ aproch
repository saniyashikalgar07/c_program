#include<stdio.h>
#include<conio.h>
int add (int N1,int N2)
{
    int Ans=0;
    Ans =N1+N2;
    return Ans;
}
int sub (int N1,int N2)
{
    int Ans=0;
    Ans =N1-N2;
    return Ans;
}
int main()
{
    int Num1=0,Num2=0,Res=0;
    printf("\n Enter 2 number for addition =");
    scanf("%d%d",& Num1 ,&Num2);
    Res=add(Num1,Num2);
    printf("\n Addition of %d&%d is =%d",Num1,Num2,Res);
    printf("\n\n Enter 2 number for sub");
    printf("%d%d",&Num1 ,&Num2);
    Res=sub(Num1,Num2);
    printf("\n sub of %d&%d is =%d",Num1,Num2,Res);
    getch();
    return 0;
}

