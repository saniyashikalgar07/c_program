#include<stdio.h>
#include<conio.h>
int main()
{
int NO=0;
printf("\n Enter any number :");
scanf("%d",&NO);

if ((NO%5==0) &&(NO%7==0))
{
    printf("\n Enter number %d is divisible by 5 as well as 7",NO);
}
else if(NO%5==0)
{
    printf("\n Enter number %d is divisible by 5",NO);
}
else if (NO%7==0)
{
    printf("\n Enter number %d is divisible by 7",NO);
}
else
{
    printf("\n Enter number %d is neither divisible by 5 not by 7 ",NO);
}
printf("\n\n =====================THANK YOU ===========================");
getch();
return 0;
}
