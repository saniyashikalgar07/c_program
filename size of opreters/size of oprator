#include<stdio.h>
#include<conio.h>
int main()
{
    int x;
    printf(" \n %d", sizeof (x));
    printf(" \n %d", sizeof (int));
    printf(" \n %d", sizeof (5));
    printf(" \n %d", sizeof (short int));
    printf(" \n %d", sizeof (long int));
    printf(" \n %d", sizeof (char));
    printf(" \n %d", sizeof (float));
    printf(" \n %d", sizeof (double));
    printf(" \n %d", sizeof (long double));
    getch();
    return 0;

}
